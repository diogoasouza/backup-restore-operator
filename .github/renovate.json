{
  "extends": [
    "github>rancher/renovate-config#release",
    "group:allNonMajor"
  ],
  "baseBranches": [
    "main",
    "release/v6.x",
    "release/v5.0",
    "release/v4.0"
  ],
  "ignoreDeps": [
    "github.com/rancher/hull",
    "github.com/rancher/lasso",
    "github.com/rancher/wrangler",
    "github.com/rancher/wrangler/v2",
    "github.com/rancher/wrangler/v3",
    "github.com/imdario/mergo"
  ],
  "packageRules": [
    {
      "groupName": "GitHub Workflow Actions",
      "groupSlug": "gha-deps",
      "matchManagers": [
        "github-actions"
      ]
    },
    {
      "groupName": "Docker File Deps",
      "groupSlug": "docker-bumps",
      "matchManagers": [
        "dockerfile"
      ]
    },
    {
      "matchBaseBranches": ["main"],
      "matchPackageNames": [
        "golang",
        "go"
      ],
      "allowedVersions": "<1.24.0"
    },
    {
      "matchBaseBranches": ["main"],
      "matchDatasources": [
        "golang-version"
      ],
      "allowedVersions": "<1.24.0"
    },
    {
      "matchBaseBranches": ["main"],
      "matchPackagePatterns": [
        "kubernetes/kubernetes"
      ],
      "allowedVersions": "<1.32.0"
    },
    {
      "matchBaseBranches": [
        "release/6.x",
        "release/5.0"
      ],
      "matchPackageNames": [
        "golang",
        "go"
      ],
      "allowedVersions": "<1.23.0"
    },
    {
      "matchBaseBranches": [
        "release/6.x",
        "release/5.0"
      ],
      "matchDatasources": [
        "golang-version"
      ],
      "allowedVersions": "<1.23.0"
    },
    {
      "matchBaseBranches": ["release/6.x"],
      "matchPackagePatterns": [
        "kubernetes/kubernetes"
      ],
      "allowedVersions": "<1.31.0"
    },
    {
      "matchBaseBranches": ["release/5.0"],
      "matchPackagePatterns": [
        "kubernetes/kubernetes"
      ],
      "allowedVersions": "<1.30.0"
    }
  ]
}